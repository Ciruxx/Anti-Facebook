{
  "manifest_version": 2,

  "name": "AntiFacebook",
  "description": "This extension will alert you when you are spending too much time on Facebook.",
  "version": "1.0",
  "background": {
        "scripts": ["background.js"]    
    },
  "icons": { "48": "noface.png",
          "128": "noface.png" },
  "browser_action": {
   "default_icon": "noface.png",
   "default_popup": "popup.html"
  },
  "permissions": [
   	"tabs",
        "storage"
   ]
}
